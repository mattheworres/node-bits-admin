{
  "name": "node-bits-admin",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel generateIndexFiles generateRootReducer && npm-run-all --parallel serve lint:watch",
    "serve": "babel-node tools/server.js",
    "lint": "esw webpack.config.* app tools --color --fix",
    "lint:watch": "npm run lint -- --watch --changed",
    "generateIndexFiles": "npm-run-all --parallel generateActionIndexFiles generateComponentIndexFiles generateServiceIndexFiles generateSelectorIndexFiles generateUtilIndexFiles generateReducerIndexFiles",
    "generateActionIndexFiles": "babel-node tools/generateActionIndexFiles.js",
    "generateComponentIndexFiles": "babel-node tools/generateComponentIndexFiles.js",
    "generateServiceIndexFiles": "babel-node tools/generateServiceIndexFiles.js",
    "generateSelectorIndexFiles": "babel-node tools/generateSelectorIndexFiles.js",
    "generateUtilIndexFiles": "babel-node tools/generateUtilIndexFiles.js",
    "generateReducerIndexFiles": "babel-node tools/generateReducerIndexFiles.js",
    "generateRootReducer": "babel-node tools/generateRootReducer.js",
    "generateFiles": "npm-run-all --parallel generateIndexFiles generateRootReducer",
    "clean": "rimraf dist && mkdir dist",
    "build": "npm run clean && npm-run-all --parallel generateIndexFiles generateRootReducer && cross-env NODE_ENV=production webpack --no-color --config webpack.config.prod.babel.js --bail"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "bootstrap-sass": "^3.3.7",
    "class-autobind": "^0.1.4",
    "classnames": "^2.2.5",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "humanize-plus": "^1.8.2",
    "immutable": "^3.8.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "numeral": "^2.0.4",
    "object-assign": "^4.1.1",
    "pluralize": "^5.0.0",
    "react": "^15.4.2",
    "react-addons-shallow-compare": "^15.4.2",
    "react-bootstrap": "^0.31.0",
    "react-bootstrap-sweetalert": "^3.0.0",
    "react-datetime": "^2.8.10",
    "react-dom": "^15.4.2",
    "react-dropzone": "^3.13.2",
    "react-fontawesome": "^1.6.1",
    "react-loading-overlay": "^0.2.4",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.0",
    "react-router-bootstrap": "^0.23.1",
    "react-router-redux": "^4.0.8",
    "react-rte": "^0.11.0",
    "react-select": "^1.0.0-rc.5",
    "react-spin": "^0.6.2",
    "react-spinkit": "^3.0.0",
    "reactcss": "^1.2.2",
    "redux": "^3.6.0",
    "redux-form": "^6.8.0",
    "redux-promise-middleware": "^4.3.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "styled-components": "^2.0.1",
    "toastr": "^2.1.2",
    "truefit-react-utils": "^0.0.5",
    "universal-cookie": "^2.0.8",
    "uuid-js": "^0.7.5"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.5",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.14.0",
    "browser-sync": "^2.18.12",
    "connect-history-api-fallback": "^1.3.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^3.4.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-watch": "^3.0.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.22.0",
    "node-dir": "^0.1.17",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.1",
    "postcss-loader": "^2.0.5",
    "redux-devtools-extension": "^2.13.2",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.7",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.12.2"
  }
}
